@model WebApplication1.Models.AnketViewModel

<form asp-action="Index" method="post">
    @for (int i = 0; i < Model.Sorular.Count; i++)
    {
        <div>
            <label>@Model.Sorular[i].SoruMetni</label>
            <textarea asp-for="Cevaplar[i]"></textarea>
        </div>
    }
    <button type="submit">Gönder</button>
</form>